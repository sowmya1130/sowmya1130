area calculation
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a, b, c;
        float d;

        a = scanner.nextInt();
        b = scanner.nextInt();
        c = scanner.nextInt();
        d = scanner.nextFloat();

        int e = a * a;
        System.out.println(e);

        int f = b * c;
        System.out.println(f);

        float g = 3.14f * d * d;
        System.out.printf("%.2f", g);
    }
}
